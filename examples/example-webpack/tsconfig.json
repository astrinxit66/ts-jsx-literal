{
  "compilerOptions": {
    "outDir": "./dist",
    "target": "es5",
    "jsx": "preserve",
    "module": "commonjs",
    "declaration": true,
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "esModuleInterop": true,
    "noImplicitAny": true
  }
}
